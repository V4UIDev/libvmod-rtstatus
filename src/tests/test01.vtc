varnishtest "Test rtstatus vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import rtstatus from "${vmod_topbuild}/src/.libs/libvmod_rtstatus.so";

	sub vcl_recv {
		error 200 "OK";

	}
	sub vcl_error {
		synthetic rtstatus.rtstatus();
		set obj.status = 200;
		return (deliver);
	}
} -start

client c1 {
       txreq -url "/"
       rxresp
       expect resp.status == 200
}
client c1 -run


